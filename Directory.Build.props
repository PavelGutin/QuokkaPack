<Project>
  <PropertyGroup>
    <Nullable>enable</Nullable>
    <!-- Optional: -->
    <!-- <WarningsAsErrors>nullable</WarningsAsErrors> -->
  </PropertyGroup>

  <!-- JavaScript SDK support for Angular project -->
